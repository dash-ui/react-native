{
  "name": "@dash-ui/react-native",
  "version": "1.0.0",
  "description": "Use your @dash-ui styles in react-native",
  "license": "MIT",
  "author": "Jared Lunde <jared.lunde@gmail.com> (https://jaredlunde.com/)",
  "homepage": "https://github.com/dash-ui/react-native#readme",
  "repository": "github:dash-ui/react-native",
  "bugs": "https://github.com/dash-ui/react-native/issues",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "files": [
    "/dist",
    "/src",
    "/types"
  ],
  "scripts": {
    "build": "lundle build",
    "check-types": "lundle check-types",
    "dev": "lundle build -f module,cjs -w",
    "format": "prettier --write \"{,!(node_modules|dist|coverage)/**/}*.{ts,js,md,yml,json}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "validate": "lundle check-types && pnpm run lint && jest --coverage"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "sideEffects": false,
  "types": "types/index.d.ts",
  "dependencies": {
    "@essentials/memoize-one": "^1.1.0",
    "@types/clone-deep": "^4.0.1",
    "@types/css-to-react-native": "^3.0.0",
    "clone-deep": "^4.0.1",
    "css-to-react-native": "^3.0.0",
    "ts-toolbelt": "^9.6.0",
    "type-fest": "^2.8.0"
  },
  "peerDependencies": {
    "@types/react-native": ">=0.20",
    "react": ">=16",
    "react-native": ">=0.20"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@swc-node/core": "^1.8.2",
    "@swc-node/jest": "^1.4.3",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "latest",
    "@types/react-native": "^0.65.16",
    "babel-jest": "^27.4.5",
    "cz-conventional-changelog": "latest",
    "eslint": "^7.32.0",
    "eslint-config-lunde": "^0.7.1",
    "husky": "latest",
    "jest": "latest",
    "lint-staged": "latest",
    "lundle": "latest",
    "prettier": "latest",
    "react": "^17.0.2",
    "react-native": "^0.65.2",
    "react-test-renderer": "^17.0.2",
    "typescript": "latest"
  },
  "keywords": [
    "dash react native",
    "react native",
    "react native styles",
    "styled components react native"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": [
      "lunde"
    ],
    "rules": {
      "testing-library/prefer-screen-queries": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist",
    "/types",
    "test",
    "*.config.js"
  ],
  "exports": {
    ".": {
      "browser": "./dist/module/index.js",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/main/index.js",
      "source": "./src/index.tsx",
      "types": "./types/index.d.ts",
      "default": "./dist/main/index.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "package.json": [
      "pnpx -y prettier-package-json --write"
    ],
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{md,yml,json}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "main",
      "next",
      "alpha"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "assets": [
            "types",
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "source": "src/index.tsx"
}
